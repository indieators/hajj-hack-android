    <provider
      android:authorities="com.nloops.hackathon"
      android:exported="false"
      android:name=".data.ElementsContentProvider"/>

    <service android:name=".data.ElementServices"/>

    <activity android:name=".MedicineActivity">
    </activity>
  </application>

</manifest>